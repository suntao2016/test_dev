package com.ccigmall.auto.test.page.pc.retailer.my;

import org.openqa.selenium.WebDriver;
import org.openqa.selenium.WebElement;
import org.openqa.selenium.support.FindBy;
import org.openqa.selenium.support.How;

import com.ccigmall.auto.test.page.SettingPage;

/**
 * @author jianping.gao
 * 我的鑫网首页
 */
public class MyIndexPage extends SettingPage{

	/**
	 * 
	 * @param driver
	 */
	public MyIndexPage(WebDriver driver) {
		super(driver);
	}
	
	/**
	 * 我的订单
	 *
	 */
	@FindBy(how = How.LINK_TEXT, using = "我的订单")
	private WebElement myOrder;
	
	/**
	 * 联系客服
	 *
	 */
	@FindBy(how = How.LINK_TEXT, using = "联系客服")
	private WebElement contactCustomerService;
	
	/**
	 * 优惠券
	 *
	 */
	@FindBy(how = How.LINK_TEXT, using = "优惠券")
	private WebElement coupon;
	
	
	/**
	 * 权限管理
	 *
	 */
	@FindBy(how = How.LINK_TEXT, using = "权限管理")
	private WebElement authorityManagement;
	
	/**
	 * 用户管理
	 *
	 */
	@FindBy(how = How.LINK_TEXT, using = "用户管理")
	private WebElement userManagement;
	
	
	/**
	 * 财务管理
	 *
	 */
	@FindBy(how = How.LINK_TEXT, using = "财务管理")
	private WebElement financeManagement;
	
	
	/**
	 * 商户信息
	 *
	 */
	@FindBy(how = How.LINK_TEXT, using = "商户信息")
	private WebElement merchantManagement;


	/**
	 * @return the myOrder
	 */
	public WebElement getMyOrder() {
		return myOrder;
	}


	/**
	 * @return the contactCustomerService
	 */
	public WebElement getContactCustomerService() {
		return contactCustomerService;
	}


	/**
	 * @return the coupon
	 */
	public WebElement getCoupon() {
		return coupon;
	}


	/**
	 * @return the authorityManagement
	 */
	public WebElement getAuthorityManagement() {
		return authorityManagement;
	}


	/**
	 * @return the userManagement
	 */
	public WebElement getUserManagement() {
		return userManagement;
	}


	/**
	 * @return the financeManagement
	 */
	public WebElement getFinanceManagement() {
		return financeManagement;
	}


	/**
	 * @return the merchantManagement
	 */
	public WebElement getMerchantManagement() {
		return merchantManagement;
	}
	
	
	
	

	
}
