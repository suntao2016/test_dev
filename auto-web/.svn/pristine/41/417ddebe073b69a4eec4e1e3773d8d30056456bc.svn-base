<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://beust.com/testng/testng-1.0.dtd" >
<suite name="回归测试" preserve-order="true" parallel="tests"
	thread-count="1">
	
	<listeners>
		<listener class-name="com.ccigmall.auto.test.reporter.HTMLReporter">
		</listener>
	</listeners>

	<parameter name="baseUrl" value=""></parameter>
	<parameter name="loginname" value=""></parameter>
	<parameter name="password" value=""></parameter>


	<test name="创建商品" preserve-order="true" thread-count="1" verbose="1">
		<classes>
			<!-- 供应商创建商品 -->
			<class
				name="com.ccigmall.auto.test.step.pc.supplier.seller.product.CreateProductStep">
			</class>
			<!-- 供应商搜索待审核商品 -->
			<class
				name="com.ccigmall.auto.test.step.pc.supplier.seller.product.SearchPendingReviewProStep">
			</class>
			<!-- 类别系统审核供应商新增商品 -->
			<class
				name="com.ccigmall.auto.test.step.pc.category.product.review.SupplierProductReviewStep">
			</class>
			<!-- wofe修改新增商品 -->
			<class
				name="com.ccigmall.auto.test.step.pc.platform.seller.product.EditProductStep">
			</class>
			<!-- 类别系统审核wofe 修改的新增商品 -->
			<class
				name="com.ccigmall.auto.test.step.pc.category.product.review.WofeProductReviewStep">
			</class>
			<!-- wofe商品上架 -->
			<class
				name="com.ccigmall.auto.test.step.pc.platform.seller.product.GoodsShelvesStep">
			</class>
			<!-- 类别系统商品授权 -->
			<class
				name="com.ccigmall.auto.test.step.pc.category.product.manage.ProductAuthorizationStep">
			</class>
			<!-- wofe商品补录库存 -->
			<class
				name="com.ccigmall.auto.test.step.pc.platform.seller.product.MakeupStockStep">
			</class>
		</classes>
	</test>
	
	<test name="品牌管理，增加，修改，删除" preserve-order="true" thread-count="1" verbose="1">
		<classes>
		    <!-- supplier 品牌增加 -->
			<class name="com.ccigmall.auto.test.step.pc.supplier.seller.brand.BrandManagementStep">
			</class>
			<!-- supplier 品牌删除 -->
			<class name="com.ccigmall.auto.test.step.pc.supplier.seller.brand.DelBrandManagementStep">
			</class>
			<!-- supplier 品牌修改 -->
			<class name="com.ccigmall.auto.test.step.pc.supplier.seller.brand.UpdateBrandManagementStep">
			</class>
			<!-- wofe 品牌审核 -->
			<class
				name="com.ccigmall.auto.test.step.pc.platform.user.review.brand.BrandAuditStep">
			</class>
		</classes>
	</test>
	<test name="品牌管理，增加 审核" preserve-order="true" thread-count="1" verbose="1">
		<classes>
		    <!-- supplier 品牌增加 -->
			<class name="com.ccigmall.auto.test.step.pc.supplier.seller.brand.BrandManagementStep">
			</class>
			<!-- wofe 品牌审核 -->
			<class
				name="com.ccigmall.auto.test.step.pc.platform.user.review.brand.BrandAuditStep">
			</class>
		</classes>
	</test>
	
	
	
	
</suite>