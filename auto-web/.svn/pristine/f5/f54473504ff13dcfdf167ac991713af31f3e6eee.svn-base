package com.ccigmall.auto.test.page.pc.category.user.rolemanager;

import org.openqa.selenium.WebDriver;
import org.openqa.selenium.WebElement;
import org.openqa.selenium.support.FindBy;
import org.openqa.selenium.support.How;

import com.ccigmall.auto.test.page.pc.category.user.UserPage;

/**
 * @author zhaixiaoyue
 * 用户管理-角色管理
 */
public class RoleManagerPage extends UserPage{
	/**
	 * 
	 * @param driver
	 */
	public RoleManagerPage(WebDriver driver) {
		super(driver);
	}
	
	/**
	 * 搜索栏角色名输入框
	 */
	@FindBy(how = How.XPATH, using = "//input[@name='rolename']")
	private WebElement roleName;
	
	
	/**
	 * 搜索按钮
	 */
	@FindBy(how = How.XPATH, using = "//div/button")
	private WebElement searchButton;
	
	/**
	 * 添加角色按钮
	 */
	@FindBy(how = How.XPATH, using = "//a[@class='user_add']")
	private WebElement addRoleBtn;
	
	/**
	 * 删除角色按钮
	 */
	@FindBy(how = How.XPATH, using = "//a[@class='user_delete']")
	private WebElement delRoleBtn;
	
	/**
	 * 修改角色按钮
	 */
	@FindBy(how = How.XPATH, using = "//a[@class='user_edit']/span[text()='修改角色']")
	private WebElement editRoleBtn;
	
	/**
	 * 授权按钮
	 */
	@FindBy(how = How.XPATH, using = "//a[@class='user_edit']/span[text()='授权']")
	private WebElement authorizeBtn;
	
	/**
	 * 添加角色-角色名输入框
	 */
	@FindBy(how = How.XPATH, using = "//form/div[1]/p/input")
	private WebElement addRoleName;
	
	
	/**
	 * 第一条记录
	 */
	@FindBy(how = How.XPATH, using = "//tbody/tr[1]/td[2]")
	private WebElement firstRole;
	
	/**
	 * 授权页面勾选-添加产品线
	 */
	@FindBy(how = How.XPATH, using = "//form/div[1]/ul/li[1]/ul/li[1]/ul/li[1]/input")
	private WebElement addRoleA;
	
	/**
	 * 授权页面勾选-属性管理
	 */
	@FindBy(how = How.XPATH, using = "//form/div[1]/ul/li[1]/ul/li[1]/ul/li[5]/input")
	private WebElement addRoleB;
	
	
	
	/**
	 * 添加或修改用户保存按钮
	 */
	@FindBy(how = How.XPATH, using = "//button[@type='submit']")
	private WebElement addSubmit;
	
	/**
	 * 添加或修改用户取消按钮
	 */
	@FindBy(how = How.XPATH, using = "//button[@class='close']")
	private WebElement addCancel;
	
	/**
	 * 提示框确定按钮
	 */
	@FindBy(how = How.XPATH, using = "//a[@class='button']/span[text()='确定']")
	private WebElement queryBtn;
	
	/**
	 * 提示框取消按钮
	 */
	@FindBy(how = How.XPATH, using = "//a[@class='button']/span[text()='取消']")
	private WebElement cancelBtn;
	
	
	/**
	 * 搜索结果为空
	 */
	@FindBy(how = How.XPATH, using = "/html/body/div[1]/div[3]/div/div[2]/div[2]/div[2]/div[2]/h2")
	private WebElement searchresult;
	
	/**
	 * 操作成功确认按钮
	 */
	@FindBy(how = How.XPATH, using = "/html/body/div[13]/div[1]/div/div[2]/ul/li/a/span")
	private WebElement query;
	
	
	
	/**
	 * @return the roleName
	 */
	public WebElement getRoleName() {
		return roleName;
	}
	
	/**
	 * @return the addRoleName
	 */
	public WebElement getAddRoleName() {
		return addRoleName;
	}
	
	/**
	 * @return the searchButton
	 */
	public WebElement getSearchButton() {
		return searchButton;
	}
	
	
	/**
	 * @return the addRoleBtn
	 */
	public WebElement getAddRoleBtn() {
		return addRoleBtn;
	}
	
	
	/**
	 * @return the delRoleBtn
	 */
	public WebElement getDelRoleBtn() {
		return delRoleBtn;
	}
	
	/**
	 * @return the editRoleBtn
	 */
	public WebElement getEditRoleBtn() {
		return editRoleBtn;
	}
	
	/**
	 * @return the authorizeBtn
	 */
	public WebElement getAuthorizeBtn() {
		return authorizeBtn;
	}
	
	/**
	 * @return the firstRole
	 */
	public WebElement getFirstRole() {
		return firstRole;
	}	
	
	/**
	 * @return the searchresult
	 */
	public WebElement getAddRoleA() {
		return addRoleA;
	}
	
	/**
	 * @return the searchresult
	 */
	public WebElement getAddRoleB() {
		return addRoleB;
	}
	
	
	
	
	/**
	 * @return the addSubmit
	 */
	public WebElement getAddSubmit() {
		return addSubmit;
	}	
	
	/**
	 * @return the addCancel
	 */
	public WebElement getAddCancel() {
		return addCancel;
	}	
	
	/**
	 * @return the queryBtn
	 */
	public WebElement getQueryBtn() {
		return queryBtn;
	}	
	
	/**
	 * @return the cancelBtn
	 */
	public WebElement getCancelBtn() {
		return cancelBtn;
	}	
	
	
	
	/**
	 * @return the searchresult
	 */
	public WebElement getSearchresult() {
		return searchresult;
	}
	
	/**
	 * @return the query
	 */
	public WebElement getQuery() {
		return query;
	}
}
