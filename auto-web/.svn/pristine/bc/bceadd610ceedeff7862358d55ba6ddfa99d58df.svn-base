	public WebElement getSaveButton() {
		return saveButton;
	}

	public WebElement getUploadImg() {
		return uploadImg;
	}

	public WebElement getCompanyName() {
		return companyName;
	}

	public WebElement getLegalPerson() {
		return legalPerson;
	}

	public WebElement getLegalPersonNo() {
		return legalPersonNo;
	}

	public WebElement getAddress() {
		return address;
	}

	public WebElement getContactor() {
