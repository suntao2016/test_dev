	private WebElement businessLicenseUrl1;
	
	/**
	 *税务登记证
	 */
	@FindBy(name="taxLicenseUrl1")
	private WebElement taxLicenseUrl1;
	
	/**
	 *开户许可证
	 */
	@FindBy(name="accountLicensesUrl1")
	private WebElement accountLicensesUrl1;
	
	/**
	 *一般纳税人资格证明
	 */
	@FindBy(name="taxpayerCertificateUrl1")
	private WebElement taxpayerCertificateUrl1;
	
	/**
	 *法人代表身份证明文件
	 */
	@FindBy(name="legalCertificateUrl1")
	private WebElement legalCertificateUrl1;
	
	/**
	 *对外贸易经营者备案登记表
	 */
