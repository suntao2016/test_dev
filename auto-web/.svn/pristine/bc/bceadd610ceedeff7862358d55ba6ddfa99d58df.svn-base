	/**
	 *食品流通许可证
	 */
	@FindBy(name="foodCirculationUrl1")
	private WebElement foodCirculationUrl1;

	/**
	 *进口食品/化妆品收货人检验检疫备案登记
	 */
	@FindBy(name="importRegistrationUrl1")
	private WebElement importRegistrationUrl1;

