	/**
	 *食品流通许可证有效期限 start
	 */
	@FindBy(id="time1")
	private WebElement startDate;
	
	/**
	 *食品流通许可证有效期限 end
	 */
	@FindBy(id="time2")
	private WebElement endDate;
	
	/**
	 *营业执照
	 */
